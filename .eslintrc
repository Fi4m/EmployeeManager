{
    "extends": "rallycoding",
    "rules": {
        "semi": 0,
        "arrow-body-style": 0,
        "no-undef": 0
    },
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "env": {
        "es6": true
    },
    "plugins": [
      "react"
    ]
}